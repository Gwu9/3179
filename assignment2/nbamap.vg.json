{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": "COVID-19 Active Cases (10 Oct 2020)",
    "width": 800,
    "height": 400,
    "projection": {"type": "albersUsa"},
    "data": {
      "url": "/america_g.json",
      "format": {"type": "topojson", "feature": "ne_110m_admin_1_states_provinces"}
    },
    "transform": [
      {
        "lookup": "ne_110m_admin_1_states_provinces",
        "from": {
          "data": {
            "url": "/data.json"
          },
          "key": "name",
          "fields": ["number"]
        }
      },
      {"calculate":"datum.number + 0", "as":"Number"}
    ],
    "mark": {"type": "geoshape"},
    "encoding": {
      "color": {
        "field": "number",
        "type": "nominal"
      },
      "tooltip": [
        {"field": "Team", "type": "nominal"},
        {"field": "name", "type": "nominal","title":"State"}
      ]
    }
  }